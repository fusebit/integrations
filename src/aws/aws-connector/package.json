{
  "name": "@fusebit-int/aws-connector",
  "version": "7.51.51",
  "description": "AWS Connector",
  "keywords": [
    "Fusebit",
    "AWS"
  ],
  "author": "Fusebit, Inc",
  "homepage": "https://fusebit.io",
  "license": "SEE LICENSE IN LICENSE",
  "main": "libc/index.js",
  "files": [
    "libc/**/*.js",
    "libc/**/*.d.ts",
    "libc/**/*.json",
    "libc/**/*.yml",
    "libc/**/*.html"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:fusebit/integrations.git"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "test": "jest --config=./jest.config.ts --passWithNoTests",
    "tsc:version": "tsc --version",
    "build": "tsc -b --pretty && cp -r src/template libc",
    "dev": "tsc --watch --pretty",
    "lint:check": "eslint . --ext .ts --color --ignore-path ../../../.eslintignore",
    "lint:fix": "eslint . --ext .ts --color --fix --ignore-path ../../../.eslintignore"
  },
  "bugs": {
    "url": "https://github.com/fusebit/integrations/issues"
  },
  "dependencies": {
    "aws-sdk": "^2.1159.0",
    "otplib": "^12.0.1",
    "superagent": "6.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@fusebit-int/framework": "^7.51.51",
    "@types/superagent": "^4.1.12",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^4.31.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^27.2.4",
    "ts-jest": "^27.0.5",
    "typescript": "~4.4.3"
