    "fusebit"
  ],
  "author": "Fusebit, Inc",
  "homepage": "https://fusebit.io",
  "license": "SEE LICENSE IN LICENSE",
  "main": "libc/index.js",
  "files": [
    "libc/*.js",
    "libc/*.d.ts",
    "libc/form/*.html",
    "libc/middleware/*.js",
    "libc/middleware/*.d.ts",
    "libc/client/*.js",
    "libc/client/*.d.js"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:fusebit/packages.git"
  },
  "scripts": {
    "test": "echo \"Error: run tests from root\" && exit 1",
    "build": "tsc -b --pretty",
    "dev": "tsc --watch --pretty",
    "tsc:version": "tsc --version",
    "prettier:fix": "prettier --write . --ignore-path ../.prettierignore",
    "prettier:check": "prettier --check . --ignore-path ../.prettierignore",
    "lint": "eslint . --ext .ts --color --ignore-path ../.eslintignore",
    "lint:fix": "eslint . --ext .ts --color --fix --ignore-path ../.eslintignore"
  },
  "bugs": {
    "url": "https://github.com/fusebit/packages/issues"
  },
  "dependencies": {
    "@koa/router": "^10.1.1",
    "@types/koa": "2.11.0",
    "http-errors": "^1.8.0",
    "koa": "^2.13.1",
    "node-mocks-http": "^1.10.1",
    "statuses": "^2.0.1",
    "superagent": "^6.1.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.83",
    "@types/koa__router": "^8.0.8",
    "@types/statuses": "^2.0.0",
    "@types/superagent": "^4.1.12",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.4.0",
    "typescript": "3.7.3"
  }
