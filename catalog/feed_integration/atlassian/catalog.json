{
  "id": "atlassian",
  "name": "Atlassian",
  "smallIcon": "#/assets/atlassian.svg",
  "largeIcon": "#/assets/atlassian.svg",
  "version": "5.2.0",
  "description": "#README.md",
  "tags": {
    "catalog": "Issues"
  },
  "resources": {
    "configureAppDocUrl": "https://developer.fusebit.io/docs/atlassian#creating-your-own-atlassian-app"
