{
  "id": "s3",
  "name": "AWS S3",
  "smallIcon": "#/assets/s3.svg",
  "largeIcon": "#/assets/s3.svg",
  "version": "5.2.0",
  "description": "#README.md",
  "tags": {
    "catalog": "Cloud"
  },
  "resources": {
    "configureAppDocUrl": "https://developer.fusebit.io/docs/aws#creating-your-own-aws-app",
    "integrationsDocUrl": "https://developer.fusebit.io/docs/aws",
    "sampleConfig": {
      "isEnabled": true,
      "isGetEnabled": true,
      "isPostEnabled": true,
      "terms": {
        "getFail": "Oops! Something went wrong and we couldn't retrieve your objects from S3.",
        "itemName": "Object",
        "properties": [{ "name": "objectName", "label": "" }]
      }
    }
  },
  "configuration": {
    "schema": "config/schema.json",
    "uischema": "config/uischema.json",
    "data": "config/defaults.json",
    "entities": {
      "awsConnector": {
        "entityType": "connector",
        "path": "connector/"
      },
      "s3Integration": {
        "entityType": "integration",
        "path": "integration/"
      }
    }
  }
}
