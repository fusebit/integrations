{
  "id": "<% this.id %>",
  "tags": {
    "fusebit.service": "Google"
  },
  "handler": "@fusebit-int/google-connector",
  "configuration": {
    "mode": {
      "useProduction": true
    },
    "scope": "openid profile https://www.googleapis.com/auth/userinfo.email https://www.googleapis.com/auth/youtube.readonly",
    "clientId": "",
