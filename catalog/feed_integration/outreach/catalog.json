{
  "id": "outreach",
  "name": "Outreach",
  "smallIcon": "#/assets/outreach.svg",
  "largeIcon": "#/assets/outreachLarge.svg",
  "version": "5.2.0",
  "description": "#README.md",
  "tags": {
    "catalog": "CRM"
  },
  "resources": {
    "configureAppDocUrl": "https://developer.fusebit.io/docs/outreach#creating-your-own-outreach-app",
    "sampleConfig": {
      "isEnabled": true
              ,
        "isGetEnabled": true,
        "isPostEnabled": false,
        "terms": {
                                "getFail": "Oops! Something went wrong and we couldn&#39;t retrieve your Prospects",
                    "itemName": "prospect",
          "properties": [
            {
              "name": "firstName",
              "label": "First Name"
            },
            {
              "name": "lastName",
              "label": "Last Name"
            }
          ]
        }
          }
  },
  "configuration": {
    "schema": "config/schema.json",
    "uischema": "config/uischema.json",
    "data": "config/defaults.json",
    "entities": {
      "outreachConnector": {
        "entityType": "connector",
        "path": "connector/"
      },
      "outreachIntegration": {
        "entityType": "integration",
        "path": "integration/"
      }
    }
  }
}
