{
  "id": "<%this.id%>",
  "tags": {},
  "handler": "@fusebit-int/salesforce-connector",
  "configuration": {
    "scope": "api offline_access",
    "tokenUrl": "/v2/account/<%global.consts.accountId%>/subscription/<%global.consts.subscriptionId%>/connector/<%this.id%>/proxy/salesforce/oauth/token",
    "authorizationUrl": "/v2/account/<%global.consts.accountId%>/subscription/<%global.consts.subscriptionId%>/connector/<%this.id%>/proxy/salesforce/oauth/authorize",
    "clientId": "<%global.consts.random%>",
    "clientSecret": "<%global.consts.random%>",
    "refreshErrorLimit": 100000,
    "refreshInitialBackoff": 100000,
    "refreshWaitCountLimit": 100000,
    "refreshBackoffIncrement": 100000,
    "accessTokenExpirationBuffer": 500
  }
}
